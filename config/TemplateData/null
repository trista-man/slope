 if (comment_data.status == &#39;trash&#39;)
 {
 str_comment += &quot;&lt;p class=&#39;text-center&#39;&gt;&lt;i&gt;Your comment is awaiting moderation&lt;/i&gt;&lt;/p&gt;&quot;;
 }
